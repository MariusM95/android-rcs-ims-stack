<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<terminal>
		<parameter description="Is Tel-URI format supported"
			 name="TelUriFormatSupported"
			 value="true"/>

		<parameter description="Classname of the user profile interface"
			 name="UserProfileInterface"
			 value="com.orangelabs.rcs.core.ims.userprofile.SettingsUserProfileInterface"/>

		<parameter description="Classname of the registraion procedure (GIBA or HTTP Digest)"
			 name="RegistrationProcedure"
			 value="com.orangelabs.rcs.core.ims.network.registration.HttpDigestRegistrationProcedure"/>

		<parameter description="Expire period for registration (in seconds)"
			 name="RegisterExpirePeriod"
			 value="3600"/>
			 
		<parameter description="IMS connection polling period (in seconds)"
			 name="ImsConnectionPollingPeriod"
			 value="30"/>
			 
		<parameter description="Ringing period (in seconds)"
			 name="RingingPeriod"
			 value="60"/>
	</terminal>

	<sipstack>
		<parameter description="Listen port number"
			 name="SipListeningPort"
			 value="5060"/>

		<parameter description="SIP transaction timeout (in seconds)"
			 name="SipTransactionTimeout"
			 value="30"/>
	</sipstack>
	
	<media>
		<parameter description="MRSP response timeout (in seconds)"
			 name="MsrpTimeout"
			 value="5"/>
	</media>	

</configuration>
